or1(a :: Bool, b :: Bool) : Bool {
    return !(!a && !b);
}

-- ESSE ARQUIVO TEM O DETALHE DO "or" SER PALAVRA RESERVAD DO C++. NÃO IREI TRATAR ISSO, POIS ISSO É O MENOR DOS MALES...

main(){
   a = true;
   b = true;
   print or1(a,b)[0];
   print '\n';
   a = true;
   b = false;
   print or1(a,b)[0];
   print '\n';
   a = false;
   b = true;
   print or1(a,b)[0];
   print '\n';
   a = false;
   b = false;
   print or1(a,b)[0];
   print '\n';
}
